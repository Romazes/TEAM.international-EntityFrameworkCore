@model Model.Warehouse
@{
    ViewData["Title"] = "Create";
}

<h1>Add new Warehouse</h1>
<form asp-action="Create">
    <div>
        <label>The Warehouse Name</label>
        <input asp-for="Name" type="text" class="form-control" placeholder="Name" required>
    </div>
    <div>
        <label>The Warehouse Address</label>
        <input asp-for="Address" type="text" class="form-control" placeholder="Address" required>
    </div>
    <button type="submit" class="btn btn-primary mt-5">
        Add Plantation
    </button>
</form>